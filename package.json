{
  "name": "txiki.js",
  "version": "0.0.0",
  "description": "Utilities for txiki.js",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "api-docs": "typedoc --tsconfig docs/tsconfig.doc.json",
    "api-docs-watch": "typedoc --tsconfig docs/tsconfig.doc.json --watch --preserveWatchOutput --logLevel Verbose",
    "deploy-api-docs": "gh-pages -d docs/api -e api -u \"github-actions-bot <support+actions@github.com>\"",
    "lint": "eslint --max-warnings 0 ."
  },
  "license": "MIT",
  "dependencies": {
    "abortcontroller-polyfill": "^1.7.5",
    "core-js": "^3.25.5",
    "esbuild": "^0.15.11",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "getopts": "^2.3.0",
    "gh-pages": "^4.0.0",
    "ipaddr.js": "^2.0.1",
    "js-md5": "^0.7.3",
    "js-sha1": "^0.6.0",
    "js-sha256": "^0.9.0",
    "js-sha3": "^0.8.0",
    "js-sha512": "^0.8.0",
    "patch-package": "^6.4.7",
    "text-encoding-utf-8": "^1.0.2",
    "urlpattern-polyfill": "^6.0.1",
    "util": "^0.12.5",
    "uuid": "^9.0.0",
    "web-streams-polyfill": "^3.2.1",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "typedoc": "^0.25.2"
